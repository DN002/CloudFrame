{
  "multipart": [
    { "apply": { "model": "cloudframe:block/cloud_pipe_center" } },

    { "when": { "north": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_arm" } },
    { "when": { "south": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_arm", "y": 180 } },
    { "when": { "east": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_arm", "y": 90 } },
    { "when": { "west": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_arm", "y": 270 } },

    { "when": { "up": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_arm", "x": 270 } },
    { "when": { "down": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_arm", "x": 90 } },

    { "when": { "filter_north": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_filter" } },
    { "when": { "filter_south": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_filter", "y": 180 } },
    { "when": { "filter_east": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_filter", "y": 90 } },
    { "when": { "filter_west": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_filter", "y": 270 } },

    { "when": { "filter_up": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_filter", "x": 270 } },
    { "when": { "filter_down": "true" }, "apply": { "model": "cloudframe:block/cloud_pipe_filter", "x": 90 } }
  ]
}
